int main()
{
    int i,j,k=2;
    for(i=1;i<=4;i++)
    {
        for(j=1;j<=5;j++)
        {
            printf("%d\t",k);
            k=k+2;
        }
        printf("\n");
    }
}
/////////////////////////////////////////////////////////////////////////
5	4	3	2	1	
5	4	3	2	1	
5	4	3	2	1	
5	4	3	2	1

#include<stdio.h>
int main()
{
    int i,j;
    for(i=1;i<=4;i++)
    {
        for(j=5;j>=1;j--)
        {
            printf("%d\t",j);
         }
        printf("\n");
    }
}
