#include<stdio.h>
int main() {
	int n = 5;
	for(int i = 1;i<=n;i++) {
		for(int j = 1;j<=n;j++) {
			if(i+j>=n+1) {
				printf("%3c",'*');
			}else {
				printf("%3c",' ');
			}
		}
		printf("\n");
	}
}
