21	22	23	24	25

// Online C compiler to run C program online
#include <stdio.h>

int main() {
  int i,j,k=1;
  for(i=1;i<=5;i++)
  {
      for(j=1;j<=5;j++)
      {
          printf("%d\t",k++);
      }
      printf("\n");
  }

    return 0;
}
///////////////////////////////////////////////////////////////////////

1	2	3	4	5	
10	9	8	7	6	
11	12	13	14	15	
20	19	18	17	16	
21	22	23	24	25

#include <stdio.h>

int main() {
    int a=0;
    
    for(int i=1; i<=5; i++){
        
        if(i%2!=0){
            for(int j=1; j<=5; j++){
                printf("%4d", ++a);
              
            }
        }else{
            for(int j=5; j>=1; j--){
                printf("%4d", a--);
            }
        }
        printf("\n");
        a=a+5;
    }

    return 0;
}
